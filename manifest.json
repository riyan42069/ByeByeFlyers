{
  "manifest_version": 3,
  "name": "ByeByeFlyers",
  "version": "1.0",
  "description": "Unsubscribe from unwanted flyers in Gmail",
  "permissions": ["identity", "identity.email"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "oauth2": {
    "client_id": "YOUR_GOOGLE_CLIENT_ID",
    "scopes": ["https://www.googleapis.com/auth/gmail.modify"]
  }
}
